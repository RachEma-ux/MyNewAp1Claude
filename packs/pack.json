{
  "pack_version": 1,
  "id": "hellonewapp",
  "name": "HelloNewApp",
  "version": "1.3.0",
  "type": "wasm",
  "entry": "hellonewapp.wasm",
  "description": "Utility Pack for MyNewAp1Claude with Fibonacci, Prime checker, Factorial, GCD, and Random number functions.",
  "permissions": {
    "network": {
      "connect": [
        "http://localhost:5000",
        "http://localhost:3000"
      ],
      "listen_localhost": false
    },
    "filesystem": {
      "read": ["data/"],
      "write": ["cache/"]
    }
  },
  "limits": {
    "memory_mb": 32,
    "cpu_ms_per_sec": 500
  },
  "build": {
    "git_sha": "hellonewapp-v1.3.0",
    "built_at": "2026-01-18T12:50:00Z",
    "target": "android-arm64"
  },
  "exports": [
    "fibonacci",
    "is_prime",
    "factorial",
    "gcd",
    "random",
    "seed_random",
    "add",
    "multiply",
    "clamp",
    "get_version"
  ]
}
